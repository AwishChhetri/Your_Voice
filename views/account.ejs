
<h1>Map Table</h1>
<table class="table table-dark .table-striped ">
    <thead class="thead-dark">
      <tr>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Age</th>
        <th scope="col">Email</th>
      </tr>
    </thead>
    <tbody>
     
        <% result.map((Result)=>{ %>
            <tr>
              <th scope="row"><%= Result.firstname %></th>
              <td><%= Result.lastname %></td>
              <td><%= Result.Age %></td>
              <td><%= Result.email %></td>
            </tr>
        <% }) %>


    </tbody>
  </table>
<h1>Filter Table</h1>
  <table class="table table-dark .table-striped ">
    <thead class="thead-dark">
      <tr>
        <th scope="col">First</th>
        <th scope="col">Last Name</th>
        <th scope="col">Age</th>
        <th scope="col">Email</th>
      </tr>
    </thead>
    <tbody>
     
      <% result.filter((Result1) => Result1.Age > 20).forEach((Result) => { %>
        <tr>
          <th scope="row"><%= Result.firstname %></th>
          <td><%= Result.lastname %></td>
          <td><%= Result.Age %></td>
          <td><%= Result.email %></td>
        </tr>
      <% }) %>


    </tbody>
  </table>

  